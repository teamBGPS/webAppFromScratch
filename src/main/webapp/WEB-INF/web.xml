<web-app>
  <servlet>
    <servlet-name>HelloHelloName</servlet-name>
    <servlet-class>org.teamBgps.demo.basicwebapp.HelloHello</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloHelloName</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
</web-app>
